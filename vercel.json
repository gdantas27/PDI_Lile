{
    "builds": [
      {
        "src": "src/index.html", // Caminho para o arquivo HTML principal
        "use": "@vercel/static" // Serve os arquivos est√°ticos diretamente
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.html" // Redireciona todas as rotas para o index.html
      }
    ]
  }